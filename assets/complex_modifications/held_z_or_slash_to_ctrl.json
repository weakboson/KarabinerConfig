{
    "title": "held z, / to control",
    "rules": [
      {
        "description": "held z to left_control",
        "manipulators": [
          {
            "type": "basic",
            "from": {
                "key_code": "z"
            },
            "parameters": {
                "basic.to_if_alone_timeout_milliseconds": 100,
                "basic.to_if_held_down_threshold_milliseconds": 100
            },
            "to_if_alone": [
                {
                    "key_code": "z"
                }

            ],
            "to_if_held_down": [
                    {
                        "key_code": "left_control"
                    }
                ]
            }
        ]
      },
      {
        "description": "held / to right_control",
        "manipulators": [
          {
            "type": "basic",
            "from": {
                "key_code": "slash"
            },
            "parameters": {
                "basic.to_if_alone_timeout_milliseconds": 100,
                "basic.to_if_held_down_threshold_milliseconds": 100
            },
            "to_if_alone": [
                {
                    "key_code": "slash"
                }

            ],
            "to_if_held_down": [
                    {
                        "key_code": "right_control"
                    }
                ]
            }
        ]
      }
    ]
  }
